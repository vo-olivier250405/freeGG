{
  "expo": {
    "name": "freeGGApp",
    "slug": "freeGGApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#000000"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow FreeGG to access your photos.",
          "savePhotosPermission": "Allow FreeGG to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  },
  "assetBundlePatterns": [
    "**/*"
  ],
  "ios": {
    "supportsTablet": true
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/adaptive-icon.png",
      "backgroundColor": "#ffffff"
    }
  },
  "web": {
    "favicon": "./assets/favicon.png"
  }
}